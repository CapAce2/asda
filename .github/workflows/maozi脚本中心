local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "maozi脚本中心┃v1.0", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroText = "maozi脚本中心"})

local jichu = Window:MakeTab({
	Name = "基础",
	Icon = "rbxassetid://7733992604",
	PremiumOnly = false
})

local daquan = Window:MakeTab({
	Name = "脚本",
	Icon = "rbxassetid://7733799795",
	PremiumOnly = false
})

jichu:AddSlider({
	Name = "速度",
	Min = 0,
	Max = 1000,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "速度",
	Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})

jichu:AddSlider({
	Name = "跳跃力量",
	Min = 0,
	Max = 1000,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "力量",
	Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})

jichu:AddButton({
	Name = "IY指令",
	Callback = function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
  	end    
})

